{
    "extends": ["plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "react/button-has-type": "warn",
        "react/prop-types": "off",
        "react/no-unescaped-entities": "warn"
    }
}
